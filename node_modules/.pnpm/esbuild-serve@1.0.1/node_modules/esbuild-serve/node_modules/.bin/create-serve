#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules/create-serve/src/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules/create-serve/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules/create-serve/src/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules/create-serve/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/create-serve@1.0.1/node_modules:/home/drnt/projects/monke/auden/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-serve@1.0.1/node_modules/create-serve/src/bin.js" "$@"
else
  exec node  "$basedir/../../../../../create-serve@1.0.1/node_modules/create-serve/src/bin.js" "$@"
fi
